				<!-- Version -->
				<input>
					<grammar>
						<item>version</item>
					</grammar>
					<output>
						<prompt selectionType="RANDOM">
							<item>Nags version 2.0</item>
						</prompt>
					</output>
				</input>
				<input>
			
		<!-- Filter bad words-->
				<grammar>
					<item>* fuck *</item>
					<item>$ fuck</item>
					<item>* shit *</item>
					<item>$ shit</item>
					<item>* rascal *</item>
					<item>* bitch *</item>
					<item>* bastard *</item>
					<item>* idiot *</item>
